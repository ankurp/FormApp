


<div class="row">
  <div class="col-sm-6">
    <h1>Forms</h1>
  </div>

  <div class="col-sm-6 text-right">
  <%= link_to new_form_path, class: 'btn btn-primary' do %>
    Add New Form
  <% end %>
  </div>
</div>

<div class="table-responsive">
  <table class="table table-striped table-bordered table-hover">
    <thead>
      <tr>
    
        <th>Name</th>
    

            
      </tr>
    </thead>

    <tbody>
      <% @forms.each do |form| %>
        <%= content_tag :tr, id: dom_id(form), class: dom_class(form) do %>
          
            <td><%= link_to form.name, form %></td>
          

          
          
        <% end %>
      <% end %>
    </tbody>
  </table>
</div>
